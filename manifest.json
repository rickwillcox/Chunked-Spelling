{
  "manifest_version": 3,
  "name": "Chunked Spelling",
  "version": "1.0",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "48": "icon48.png"
    }
  },
  "permissions": ["activeTab", "scripting", "storage"],
  "icons": {
    "48": "icon48.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
